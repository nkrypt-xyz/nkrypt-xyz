{
  "name": "nkrypt-web-client",
  "version": "2.0.0",
  "description": "nkrypt.xyz encrypted file storage web client",
  "productName": "nkrypt.xyz",
  "author": "nkrypt.xyz",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./src",
    "lintfix": "eslint --fix --ext .js,.ts,.vue ./src",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "npm run check && quasar build",
    "build-spa": "quasar build -m spa",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "type-check:watch": "vue-tsc --noEmit --watch --skipLibCheck",
    "type-check:strict": "vue-tsc --noEmit --project tsconfig.typecheck.json",
    "check": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "pinia": "^2.1.4",
    "quasar": "^2.6.0",
    "streamsaver": "^2.0.6",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@types/node": "^12.20.21",
    "@types/streamsaver": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.32.12",
    "typescript": "^4.5.4",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "^22 || ^20",
    "npm": ">= 6.13.4"
  }
}
